generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // Aquí estaba el error. Ahora usaremos el nombre que sí tiene en su .env:
  url      = env("POSTGRES_URL") 
}

model Lead {
  id        String   @id @default(cuid())
  email     String   @unique
  createdAt DateTime @default(now())
  source    String?
}