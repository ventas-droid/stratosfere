generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_URL")       // <--- ¡AHORA SÍ!
  directUrl = env("POSTGRES_URL")
}

// ¡Esta es la parte que Vercel dice que falta!
model Lead {
  id        String   @id @default(cuid())
  email     String   @unique
  createdAt DateTime @default(now())
  source    String?
}


